{
  "extends": [
    "tslint-ionic-rules/strict",
    // "tslint-ionic-rules",
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-eslint-rules"
  ],
  "rules": {
    // Permitir el uso de console.
    "no-console": false,
    // Maximo de caracteres por linea
    "max-line-length": [
      true,
      172
    ]
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}